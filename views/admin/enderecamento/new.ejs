<%- include('../../partials/header.ejs')%>

    <body>
        <%- include('../../partials/navbar.ejs')%>
            <div class="container">
                <div class="card-header">
                    <h3>Cadastro do Enderecamento</h3>
                </div>
                <div class="card-body">
                    <form method="POST" action="/enderecamento/save">
                        
                        <select name="tabelaId">
                            <%tabelas.forEach(tabela =>{%>
                            <option value="<%=tabela.id%>"><%=tabela.nome_rua%></option>
                            <%})%>
                        </select>
                        <div class="card-footer">
                            <button class="btn btn-success">Cadastrar</button>
                        </div>

                    </form>
                </div>
            </div>
    </body>
    <%- include('../../partials/footer.ejs')%>